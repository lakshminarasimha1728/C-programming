#include<stdio.h>
void main()
{
    int rem,n,x,rev=0;
    printf("enter any number:");
    scanf("%d",&n);
    x=n;
    while(n>0)
    {
        rem=n%10;
        rev=rev*10+rem;
        n=n/10;
    }
    if(x==rev)
    printf("given number is pallindrom");
    else
    printf("given number is not pallindrom");
}
