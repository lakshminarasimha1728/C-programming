#include<stdio.h>
void main()
{
    int rem,n,x,arm=0;
    printf("enter any number:");
    scanf("%d",&n);
    x=n;
    while(n>0)
    {
        rem=n%10;
        arm=arm+(rem*rem*rem);
        n=n/10;
    }
    if(x==arm)
    printf("given number is Armstrong");
    else
    printf("given number is not Armstrong");
}
